{
  "name": "jsgame",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "10.16.0",
    "npm": "6.9.0"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "sass-lint": "sass-lint -v",
    "back": "json-server --host=$(ifconfig | grep 'inet ' | grep -v 127.0.0.1 | cut -d ' '  -f2) data/db.json",
    "lint:fix": "sass-lint-auto-fix --config-sass-lint ./.sass-lint.yml",
    "start": "npm run serve"
  },
  "dependencies": {
    "firebase": "^5.8.0",
    "json-server": "^0.16.1",
    "sass-loader": "^7.1.0",
    "vue": "^2.5.21",
    "vue-fetch": "^1.1.2",
    "vue-router": "^3.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-service": "^3.5.1",
    "axios": "^0.18.1",
    "sass": "^1.26.7",
    "sass-lint": "^1.12.1",
    "sass-lint-auto-fix": "^0.21.0",
    "vue-template-compiler": "^2.5.21",
    "vuex": "^3.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
